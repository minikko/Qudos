    <script>
    AOS.init();

    function clipcode(code, btn) {
        /* Get the text field */
        var copyText = document.getElementById(code);
        //var dummy = $('<input>').val(code).appendTo('body2').select()
        //document.execCommand('copy');
        code.select();
        code.setSelectionRange(0, 99999);
        document.execCommand("copy");
        //var c = document.getElementById(code);
        btn.innerHTML = '<i class="fas fa-check"></i> &nbsp;Copied to clipboard.';
        btn.setAttribute('style', 'color:rgba(0,0,0,.8)');

        setTimeout(() => {
            btn.setAttribute('style', 'color:rgba(0,0,0,.5)');
            btn.innerHTML = '<i class="far fa-copy"></i> &nbsp;Copy to clipboard';
        }, 2000);

    }
    </script>
